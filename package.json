		"package": "vsce package -o ./bin/dracula.vsix",
		"vscode:prepublish": "npm run build"
	},
	"maintainers": [
		"Florian Duros <florian.duros@gmail.com>"
	],
	"contributors": [
		"Florian Duros"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/florianduros/dracula-italics-visual-studio-code.git"
	},
	"bugs": {
		"url": "https://github.com/florianduros/dracula-italics-visual-studio-code.git"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dracula",
		"dark",
		"theme",
		"color-theme",
		"italic"
	],
	"icon": "icon.png",
