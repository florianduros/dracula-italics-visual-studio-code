{
	"name": "theme-dracula-italics-turbo",
	"version": "1.0.2",
	"displayName": "Dracula Italics Turbo",
	"homepage": "https://draculatheme.com/",
	"description": "Forked version of dracula enhanced with italic keywords",
	"publisher": "FlorianDuros",
	"license": "MIT",
	"scripts": {
		"dev": "node ./scripts/dev.js",
		"build": "node ./scripts/build.js",
